#include <iostream>

using namespace std;

float averageFinder(){
    int size = 0;
    int sum = 0;
    cin>>size;
    int numbers[size]{0};
    for (int i=0;i<size;i++){
        cin>>numbers[i];
        sum+=numbers[i];
    }
    return float(sum)/float(size);
}
int main()
{
    cout<<averageFinder()<<endl;`
    return 0;
}
