#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    float a = 0;
    float b = 0;
    float c = 0;
    float x1 = 0;
    float x2 = 0;
    
    cin>>a>>b>>c;
    x1 = (-b+sqrt(b*b-4*a*c))/(2*a);
    x2 = (-b-sqrt(b*b-4*a*c))/(2*a);
    if (x1==x2){
        cout<<"x1=x2="<<x1;
    } else {
        cout<<"x1="<<x1<<" and x2="<<x2;
    }
    return 0;
}
