#include <iostream>
#include <cmath>
using namespace std;
int main()
{ 
    int num1 = 0;
    int num2 = 0;
    int num3 = 0;
    cin>>num1>>num2>>num3;
    if (num3%num1==0 && num3%num2==0){
        cout<<num1<<" and "<<num2<<" are factors of "<<num3;
    } else{
        cout<<num1<<" and "<<num2<<" are not factors of "<<num3;
    }
    return 0;
}
