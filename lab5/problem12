#include <iostream>
#include <string>
#include <cmath>
using namespace std;

int main()
{
    string num;
    int number = 0;
    int result = 0;
    cin>>num;
    for (int i=0;i<num.length();i++){
        number = num[i]-'0';
        result += number*pow(2,(num.length()-i-1));
    }
    cout<<"The decimal number is "<<result;
    return 0;
}
