#include <iostream>

using namespace std;

int main()
{
    char letter;
    cin>>letter;
    if (int(letter)>=65 && int(letter)<=90){
        cout<<"Uppercase alphabet";
    } else if (int(letter)>=97 && int(letter)<=122){
        cout<<"Lowercase alphabet";
    } else{
        cout<<"It is not an alphabet";
    }
    
    return 0;
}
